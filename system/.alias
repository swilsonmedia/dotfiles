alias h="cd ~"
alias ..='cd ../'                           
alias ...='cd ../../' 
alias mysql=/usr/local/mysql/bin/mysql
alias server="python -m SimpleHTTPServer 8000"

alias edit="$EDITOR SPEWeb/ SmartPak.DesignSystem/ SmartPak.Javascript.Test/ && svn update && clear"

alias status="svn status"
alias update="svn update"
alias revert="svn revert . -R"
alias add="svn add"
alias addall="status | grep '?' | cut -d '?' -f2 | xargs svn add"
alias delete="svn delete"
alias deleteall="status | grep '!' | cut -d '!' -f2 | xargs svn delete"
alias addremove="addall && deleteall"
alias clean="svn status | grep edited | cut -d '?' -f2 | xargs rm"
alias diff="svn diff --diff-cmd ~/dotfiles/scripts/svn-diffwrap.sh" 
alias uglify="uglifyjs --compress expression=true --mangle -- "
alias minify="html-minifier --collapse-whitespace --remove-comments --remove-tag-whitespace --minify-css true --minify-js true "
alias wba="npx webpack --mode production --profile --json > stats.json && npx webpack-bundle-analyzer stats.json";

alias nocert="/Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome --ignore-certificate-errors &> /dev/null &"